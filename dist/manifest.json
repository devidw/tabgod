{
    "manifest_version": 3,
    "name": "tabgod",
    "description": "Control them all.",
    "version": "12024.02.10",
    "options_page": "./dev.html",
    "permissions": [
        "tabs",
        "scripting"
    ],
    "background": {
        "service_worker": "sw.js"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "externally_connectable": {
        "matches": [
            "<all_urls>"
        ]
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}