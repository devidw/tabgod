{
    "manifest_version": 3,
    "name": "Tabgod: Cross-tab JavaScript execution",
    "description": "Easily execute JavaScript across multiple chromium tabs in parallel.",
    "version": "12024.02.11",
    "options_page": "./dev.html",
    "permissions": [
        "tabs",
        "scripting"
    ],
    "background": {
        "service_worker": "sw.js"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "externally_connectable": {
        "matches": [
            "<all_urls>"
        ]
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}