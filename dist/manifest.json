{
    "manifest_version": 3,
    "name": "tabgod",
    "description": "Control them all.",
    "version": "12024.02.07",
    "permissions": [
        "tabs",
        "scripting"
    ],
    "background": {
        "service_worker": "sw.js"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "externally_connectable": {
        "matches": [
            "<all_urls>"
        ]
    },
    "icons": {
        "16": "tabgod.png",
        "48": "tabgod.png",
        "128": "tabgod.png"
    }
}